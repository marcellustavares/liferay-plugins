<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.portal.dynamicstorage.model.DynamicStorageAppEntity" />
	<import class="com.liferay.portal.dynamicstorage.model.DynamicStoragePersistence" />
	<import class="com.liferay.portal.dynamicstorage.model.DynamicStorageRegistry" />
	<class name="com.liferay.portal.dynamicstorage.model.impl.DynamicStorageAppEntityImpl" table="DynamicStorageAppEntity">
		<cache usage="read-write" />
		<id name="appEntityId" type="long">
			<generator class="assigned" />
		</id>
		<property name="appId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="entityName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="DDMStructureId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>